<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN" name="MyHandler">
    <Appenders>
        <Console name="OUT">
            <PatternLayout>
            <Pattern>%d [%t] (%M) %m%n</Pattern>
            </PatternLayout>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="all">
            <AppenderRef ref="OUT"/>
        </Root>
    </Loggers>
</Configuration>